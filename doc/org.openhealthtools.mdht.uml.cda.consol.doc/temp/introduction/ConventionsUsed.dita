    <b class="+ topic/ph hi-d/b " xtrf="file:/Users/seanmuir/git/mdht-models/doc/org.openhealthtools.mdht.uml.cda.consol.doc/dita/introduction/ConventionsUsed.dita" xtrc="b:13;67:8">STATIC</b>
